{
  "name": "newsmicroservice",
  "version": "1.0.0",
  "description": "A backend news API microservice to show COVID-19 related news",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=dev nodemon -r dotenv/config server.js",
    "start_windows": "set NODE_ENV=dev&&node -r dotenv/config server.js",
    "test": "jest --setupFiles dotenv/config -i --testTimeout=15000 --forceExit",
    "prod": "NODE_ENV=prod node -r dotenv/config server.js"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "keywords": [],
  "author": "G2T4",
  "private": true,
  "license": "UNLICENSED",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^6.0.11",
    "morgan": "^1.10.0",
    "newsapi": "^2.4.1",
    "node-cron": "^3.0.0",
    "rss-parser": "^3.12.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.13"
  }
}
